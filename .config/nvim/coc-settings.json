{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    },
  "jedi": {
    "enable": true,
    "startupMessage": false,
    "executable.command": "~/.anyenv/envs/pyenv/versions/neovim3/bin/jedi-language-server"
  }
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "rust"
  ],
  "python.jediEnabled": true
}

